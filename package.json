{
  "private": true,
  "version": "0.0.0",
  "devDependencies": {
    "concurrently": "^5.2.0",
    "husky": "^4.2.5",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1"
  },
  "scripts": {
    "dev": "node_modules/.bin/concurrently \"npm run client-dev\" \"npm run server-dev-build\" \"npm run server-run-watch\"",
    "server-run": "node dist/index.js",
    "server-run-watch": "node_modules/.bin/nodemon dist/index.js",
    "server-dev-build": "cd server && node_modules/typescript/bin/tsc -w",
    "server-dist-build": "cd server && node_modules/typescript/bin/tsc",
    "client-dev": "cd client && npm run start",
    "eslint-server": "cd server && node_modules/.bin/eslint src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "mongoose": "^5.9.21",
    "mongoose-unique-validator": "^2.0.3"
  }
}
